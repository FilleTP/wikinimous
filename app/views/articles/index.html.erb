<h1>Articles</h1>
<div class="container">
<ul>
  <% @articles.each do |article| %>
  <div class="list">
    <li class="links"><%= link_to article.title, article_path(article)%></li>
    <button><%= link_to "Update", edit_article_path(article) %></button>
    </div>
  <% end %>
</ul>
</div>

<%= link_to "New Article", new_article_path %>
